{
    "manifest_version": 2,
    "name": "SRS Attendance Calculator",
    "description": "Automatically calculates missed hours",
    "version": "1.0.0",
    "permissions":["activeTab"],
    "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://stars.bilkent.edu.tr/srs/*"
      ],
      "js": ["jquery.js", "main.js"]
    }
   ]
}